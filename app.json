{
  "expo": {
    "name": "Object-Transfer-App",
    "slug": "Object-Transfer-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "objecttransferapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyChaMafk-5r9cAYmf0VBWUbBowuuaANh6I"
      },
      "bundleIdentifier": "com.anonymous.ObjectTransferApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.ObjectTransferApp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyChaMafk-5r9cAYmf0VBWUbBowuuaANh6I"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Object Transfer System to use your location to provide location-based services.",
          "locationAlwaysPermission": "Allow Object Transfer System to use your location in the background.",
          "locationWhenInUsePermission": "Allow Object Transfer System to use your location for location-based services."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
